@using CarInsurance.Models
@model List<Insuree>

<div class="row no-gutters m-3">
  <div class="card col-sm-12 col-md-10 col-lg-6 mx-auto shadow">
    <div class="card-header text-center">
      <h5 class="m-0">Insurees</h5>
    </div>
    <div class="card-body">
      @foreach (var insured in Model)
        {
          <div class="row">
            <div class="col">
              @insured.FirstName
            </div>
            <div class="col">
              @insured.LastName
            </div>
            <div class="col">
              @insured.EmailAddress
            </div>
            <div class="col">
              @insured.Quote
            </div>
          </div>
        }
    </div>
  </div>
</div>
